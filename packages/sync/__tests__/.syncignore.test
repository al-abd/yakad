# =============================================================================
# COMPLEX PATTERNS WITH WILDCARDS
# =============================================================================

# All minified files
*.min.js
*.min.css
*.min.js.map
*.min.css.map

# All source maps
*.map

# All temporary files
temp/*
tmp/*
.tmp/*
.temp/*

# All backup files
backup/*
*.backup
*.old

# =============================================================================
# NEGATION PATTERNS (EXCEPTIONS)
# =============================================================================

# Ignore all .min.js files but allow specific ones
!important.min.js
!critical.min.js
!vendor.min.js

# Ignore all test files but allow specific ones
!src/components/Button.test.tsx
!src/utils/helpers.test.ts

# Ignore all .env files but allow example
!.env.example
!.env.sample

# Ignore all in src/app but allow specific files
!/src/app/error.tsx
!/src/app/head.tsx
!/src/app/loading.tsx
!/src/app/not-found.tsx
!/src/app/global-error.tsx

# Ignore all in public but allow specific files
!/public/favicon.ico
!/public/robots.txt
!/public/site.webmanifest

