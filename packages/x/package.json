{
    "name": "@yakad/x",
    "version": "1.1.19",
    "private": false,
    "author": "Natiq",
    "description": "Extreme components for NextJS",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "yakad",
        "react",
        "react-component"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NatiqQuran/yakad.git",
        "directory": "packages/x"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NatiqQuran/yakad/issues"
    },
    "homepage": "https://yakad.natiq.net",
    "scripts": {
        "clean": "rimraf dist/",
        "copy-files": "copyfiles -u 1 src/**/*.css dist/",
        "build": "npm run clean && tsc && npm run copy-files",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src/**/*.{ts,tsx}",
        "test": "echo \"No tests specified\" && exit 0",
        "i:ui": "npm install @yakad/ui",
        "i:symbols": "npm install @yakad/symbols",
        "prepublishOnly": "npm run i:ui && npm run i:symbols && npm run build && npm run test && npm run lint && npm run typecheck",
        "newpublish": " npm version patch && npm publish"
    },
    "dependencies": {
        "@yakad/ui": "1.3.83",
        "@yakad/symbols": "1.0.24",
        "classnames": "2.5.1"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@types/css-modules": "1.0.5",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "copyfiles": "2.4.1",
        "rimraf": "6.0.1",
        "typescript": "^5.0.0"
    }
}
