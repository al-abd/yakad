{
    "name": "@yakad/eslint-plugin-guard",
    "version": "0.1.0",
    "private": false,
    "description": "ESLint plugin for Yakad development guidelines",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src/**/*.{ts,tsx}",
        "test": "echo \"No tests specified\" && exit 0",
        "newpublish": " npm version patch && npm publish"
    },
    "keywords": [
        "eslint",
        "plugin",
        "yakad",
        "guard"
    ],
    "author": "Natiq",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.3.0"
    },
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/natiq-foundation/yakad.git",
        "directory": "packages/eslint-plugin-guard"
    },
    "bugs": {
        "url": "https://github.com/natiq-foundation/yakad/issues"
    },
    "homepage": "https://yakad.natiq.net",
    "peerDependencies": {
        "eslint": ">=8.0.0 <9.0.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./configs/components": {
            "types": "./dist/configs/components.d.ts",
            "require": "./dist/configs/components.js",
            "default": "./dist/configs/components.js"
        }
    }
}
