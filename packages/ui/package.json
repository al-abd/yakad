{
    "name": "@yakad/ui",
    "version": "1.3.118",
    "private": false,
    "author": "Natiq",
    "description": "UI components for NextJS/React",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "keywords": [
        "yakad",
        "react",
        "react-component"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/natiq-foundation/yakad.git",
        "directory": "packages/ui"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/natiq-foundation/yakad/issues"
    },
    "homepage": "https://yakad.natiq.net",
    "scripts": {
        "clean": "rimraf dist/",
        "copy-files": "copyfiles -u 1 src/*.css src/**/*.css dist/",
        "build": "npm run clean && tsc && npm run copy-files",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src/**/*.{ts,tsx}",
        "test": "echo \"No tests specified\" && exit 0",
        "prepublishOnly": "npm run test && npm run build && npm run typecheck",
        "newPublish": "npm version patch && npm publish"
    },
    "dependencies": {
        "@yakad/lib": "1.0.22",
        "@yakad/symbols": "1.0.25",
        "classnames": "2.5.1"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "devDependencies": {
        "@types/css-modules": "1.0.5",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "@types/jest": "^30.0.0",
        "@typescript-eslint/eslint-plugin": "^6",
        "@typescript-eslint/parser": "^6",
        "copyfiles": "^2.4.1",
        "eslint": "^8.0.0 <9.0.0",
        "eslint-plugin-react": "^7",
        "eslint-plugin-react-hooks": "^4",
        "jest": "^30.1.2",
        "rimraf": "^6",
        "ts-jest": "^29.4.1",
        "typescript": "<5.4.0"
    }
}
