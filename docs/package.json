{
    "name": "yakad-docs",
    "version": "0.1.0",
    "private": true,
    "author": "Natiq",
    "description": "Yakad Docs",
    "scripts": {
        "sync": "yakad-sync -r https://github.com/natiq-foundation/yakad-template.git -v",
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "link:lib": "npm link --prefix ../packages/lib && npm link @yakad/lib",
        "link:ui": "npm link --prefix ../packages/ui && npm link @yakad/ui",
        "link:x": "npm link --prefix ../packages/x && npm link @yakad/x",
        "link:all": "npm run link:lib && npm run link:ui && npm run link:x",
        "ldev": "npm run link:all && npm run dev",
        "unlink:lib": "npm unlink @yakad/lib && npm i @yakad/lib",
        "unlink:ui": "npm unlink @yakad/ui && npm i @yakad/ui",
        "unlink:x": "npm unlink @yakad/x && npm i @yakad/x",
        "unlink:all": "npm run unlink:lib && npm run unlink:ui && npm run unlink:x",
        "idev": "npm run unlink:all && npm i && npm run dev"
    },
    "dependencies": {
        "@yakad/lib": "^1.0.22",
        "@yakad/symbols": "1.0.25",
        "@yakad/ui": "^1.3.125",
        "@yakad/x": "^1.1.30",
        "classnames": "^2.5.1",
        "next": "15.5.2",
        "react": "19.1.0",
        "react-dom": "19.1.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@yakad/sync": "1.0.6",
        "eslint": "^9",
        "eslint-config-next": "15.1.4",
        "typescript": "^5"
    }
}
